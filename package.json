		"@wordpress/icons": "^10.4.0",
		"@wordpress/keycodes": "^4.4.0",
		"@wordpress/primitives": "^4.4.0",
		"classnames": "^2.5.1",
		"html-react-parser": "^5.1.12"
	},
	"devDependencies": {
		"@wordpress/scripts": "^28.4.0",
